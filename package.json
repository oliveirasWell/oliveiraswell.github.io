{
  "name": "@oliveiraswell/cra-template-gh-pages",
  "version": "1.0.0",
  "homepage": "https://oliveiraswell.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliveiraswell/cra-template-gh-pages"
  },
  "license": "MIT",
  "files": [
    "template",
    "template.json"
  ],
  "engines": {
    "node": ">=14",
    "yarn": ">=1.11"
  },
  "main": "template.json",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@tecsinapse/eslint-config-tecsinapse-js": "^3.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write",
      "git add"
    ]
  }
}
