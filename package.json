{
  "name": "oliveiraswell.github.io",
  "version": "0.1.0",
  "homepage": "https://oliveiraswell.github.io",
  "private": true,
  "repository": {
    "type" : "git",
    "url" : "https://github.com/oliveiraswell/oliveiraswell.github.io.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "eslint:fix": "eslint ./src --ext .js --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@tecsinapse/eslint-config-tecsinapse-js": "^3.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write",
      "git add"
    ]
